otal = () => {\r\n        let total = 0\r\n        carrito.forEach(item => total += item.cantidad * item.price)\r\n        return total\r\n    }\r\n\r\n    \r\n    const valorDelContexto = \r\n        {\r\n            carrito: carrito,\r\n            addItem: addItem,\r\n            calcCantidad: calcCantidad,\r\n            calcTotal: calcTotal,\r\n            clear: clear\r\n            \r\n        }\r\n\r\n    return (\r\n        <Provider value={valorDelContexto}>\r\n            {children}\r\n        </Provider>\r\n    )\r\n    \r\n}\r\n\r\n\r\nexport default MiProvider","C:\\Users\\pagnu\\OneDrive\\Desktop\\React JS\\fpagnutt\\React\\src\\components\\CartWidget.js",[],[],"C:\\Users\\pagnu\\OneDrive\\Desktop\\React JS\\fpagnutt\\React\\src\\components\\ItemListContainer.js",[],[],"C:\\Users\\pagnu\\OneDrive\\Desktop\\React JS\\fpagnutt\\React\\src\\components\\Item.js",["55"],[],"C:\\Users\\pagnu\\OneDrive\\Desktop\\React JS\\fpagnutt\\React\\src\\Main.js",[],[],"C:\\Users\\pagnu\\OneDrive\\Desktop\\React JS\\fpagnutt\\React\\src\\components\\Cart.js",["56"],[],"import { useContext } from \"react\"\r\nimport { contexto } from \"../context/CartContext\"\r\n\r\n\r\nconst Cart = () => {\r\n  \r\n  const {carrito, calcTotal} = useContext(contexto)\r\n  \r\n\r\n  console.log(carrito)\r\n \r\n  return (\r\n    <div id=\"main\">\r\n      <div className=\"carritoMain\">\r\n      {carrito.map((cart) => {\r\n            return (\r\n                <article key={cart.id} className=\"cont\">\r\n                  <img  className=\"imageItem\" src={cart.image}></img>\r\n                  <div className=\"titleItem\">{cart.title}</div>\r\n                  <div>Precio: ${cart.price}</div>\r\n                  <h3>Cantidad: {cart.cantidad}</h3>\r\n                  <button>ELIMINAR</button>\r\n                </article>  \r\n               )\r\n        })}\r\n      \r\n      </div> \r\n      <div className=\"paddingTop\"><h1>TOTAL: $ {calcTotal()}</h1></div> \r\n    </div>\r\n 